{
  "name": "emarsys-integration-client",
  "version": "1.5.0",
  "description": "Client library for Emarsys services.",
  "main": "dist/index.js",
  "scripts": {
    "test": "gulp test",
    "build": "rm -rf dist && babel lib --ignore *.spec.js --out-dir dist",
    "prepublish": "npm test && npm run build",
    "release-patch": "npm version patch -m 'chore(package): update version to %s'",
    "release-minor": "npm version minor -m 'chore(package): update version to %s'",
    "release-major": "npm version major -m 'chore(package): update version to %s'"
    "code-style": "eslint $(find ./lib -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./dist/*\")",
    "validate-commit-msg": "validate-commit-msg",
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "q": "1.4.1"
  },
  "pre-commit": [
    "code-style",
    "validate-commit-msg"
  ],
  "devDependencies": {
    "babel": "5.8.23",
    "boar-tasks": "2.12.0",
    "chai": "3.3.0",
    "eslint": "2.7.0",
    "eslint-config-emarsys": "4.0.0",
    "gulp": "3.9.0",
    "gulp-mocha": "2.1.3",
    "jquery": "2.1.4",
    "lodash": "4.9.0",
    "mocha": "2.3.3",
    "pre-commit": "1.1.2",
    "run-sequence": "1.1.4",
    "sinon": "1.17.1",
    "sinon-chai": "2.8.0",
    "validate-commit-message": "3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emartech/emarsys-integration-client-js.git"
  },
  "bugs": {
    "url": "https://github.com/emartech/emarsys-integration-client-js/issues"
  },
  "homepage": "https://github.com/emartech/emarsys-integration-client-js#readme"
}
