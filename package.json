{
  "name": "emarsys-integration-client",
  "version": "1.0.1",
  "description": "Client library for Emarsys services.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha $(find lib -name \"*.spec.js\")",
    "build": "rm -rf dist && rsync -rv --exclude=*.spec.js lib/client/* dist",
    "prepublish": "npm test && npm run build",
    "release-patch": "npm version patch -m 'chore(package): update version to %s'",
    "release-minor": "npm version minor -m 'chore(package): update version to %s'",
    "release-major": "npm version major -m 'chore(package): update version to %s'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "extend": "^3.0.0"
  },
  "devDependencies": {
    "boar-tasks": "^2.12.0",
    "chai": "^3.3.0",
    "gulp": "^3.9.0",
    "gulp-mocha": "^2.1.3",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "run-sequence": "^1.1.4",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emartech/emarsys-integration-client-js.git"
  },
  "bugs": {
    "url": "https://github.com/emartech/emarsys-integration-client-js/issues"
  },
  "homepage": "https://github.com/emartech/emarsys-integration-client-js#readme"
}
